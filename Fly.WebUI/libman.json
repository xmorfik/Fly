{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "leaflet@1.9.3",
      "destination": "wwwroot/lib/leaflet/",
      "files": [
        "leaflet.min.js",
        "leaflet.min.css"
      ]
    },
    {
      "library": "bootstrap-icons@1.10.3",
      "destination": "wwwroot/lib/bootstrap-icons/"
    },
    {
      "library": "microsoft-signalr@7.0.3",
      "destination": "wwwroot/lib/microsoft-signalr/"
    },
    {
      "library": "leaflet.markercluster@1.5.3",
      "destination": "wwwroot/lib/leaflet.markercluster/",
      "files": [
        "leaflet.markercluster.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "leaflet-rotatedmarker@0.2.0",
      "destination": "wwwroot/lib/leaflet-rotatedmarker/",
      "files": [
        "example.html",
        "leaflet.rotatedMarker.js",
        "LICENSE",
        "package.json",
        "README.md"
      ]
    }
  ]
}